            <Button layoutX="424.0" layoutY="186.0" mnemonicParsing="false" onAction="#BackHome" text="Back" />
            <Label layoutX="258.0" layoutY="211.0" />
            <Button fx:id="booking" layoutX="248.0" layoutY="186.0" mnemonicParsing="false" onAction="#bookingScreen" prefHeight="31.0" prefWidth="117.0" text="Add Booking" />
         </children>
      </AnchorPane>
   </bottom>
